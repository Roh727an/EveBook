<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>AdminPannel</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-QWTKZyjpPEjISv5WaRU9OFeRpok6YctnYmDr5pNlyT2bRjXh0JMhjY6hW+ALEwIH" crossorigin="anonymous">
    <link rel="stylesheet" href="/adminPannel.css">
</head>
<body>
  <main>
  <h1 class="text-center pt-2">Welcome Admin</h1>
  <!-- User Details -->
  <h2 class="text-center text-warning bg-secondary my-5 py-2">All User Records </h2>
  <div class="container">
        <% fetchUserData.forEach(function(user) { %>
          <div class="box">
              <div class="card  mb-3" style="max-width: 18rem;">
                  <div class="card-header text-center">
                    <h6 class="card-title"><br></h6>
                  </div>
                  <div class="card-body">
                    <p class="card-text"><b>UserName: </b><%= user.userName %></p>
                    <p class="card-text"><b>Email: </b><%= user.email %></p>
                    <p class="card-text"><b>Contact: </b><%= user.contact %></p>
                  </div>
                </div>
          </div>
        <% }) %>
    </div>





  <!-- Event Details -->
  <h2 class="text-center text-warning bg-secondary my-5 py-2">All Events Records </h2>
  <div class="container event">
        <% fetchEventData.forEach(function(event) { %>
          <div class="box">
              <div class="card  mb-3" style="max-width: 18rem;">
                  <div class="card-header text-center">
                    <h6 class="card-title"><br></h6>
                  </div>
                  <div class="card-body">
                    <p class="card-text"><b>Email: </b><%= event.userEmail %></p>
                    <p class="card-text"><b>Event Name: </b><%= event.eventName %></p>
                    <p class="card-text"><b>Event Venue: </b><%= event.eventVenue %></p>
                    <p class="card-text"><b>Event City: </b><%= event.city %></p>
                    <p class="card-text"><b>Payment Status: </b>Done</p>
                  </div>
                </div>
          </div>
        <% }) %>
      </div>
      <button type="button" class="btn btn-outline-warning logout my-3"><a href="/index"><b>Home</b></a></button>
    </main>
      <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-YvpcrYf0tY3lHB60NNkmXc5s9fDVZLESaAA55NDzOxhy9GkcIdslK1eN7N6jIeHz" crossorigin="anonymous"></script>
</body>

</html>